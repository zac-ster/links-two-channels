{
  "name": "Links two channels",
  "description": "A simple discord bot that use webhook to send message from a channel to the other channel",
  "repository": "https://github.com/zac-ster/links-two-channels",
  "logo": "https://avatars.githubusercontent.com/u/60801799?s=120&v=4",
  "keywords": [
    "python", 
    "flask", 
    "webhook"
  ],
  "image": "heroku/python",
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "ChannelId1": {
      "description": "First channel id.",
      "required": "true"
    },
    "ChannelId2": {
      "description": "Second channel id.",
      "required": "true"
    },
    "Webhook1": {
      "description": "First channel webhook url.",
      "required": "true"
    },
    "Webhook2": {
      "description": "Second channel webhook url.",
      "required": "true"
    },
    "Token": {
      "description": "Your bot's token",
      "required": "true"
    }
  }
}
